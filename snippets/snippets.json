	"anatomy": {
		"prefix": "anatomy",
		"body": "\n<Container>\n\t<Header>\n\t\t<Button>\n\t\t\t<Icon name=\"$1\"/>\n\t\t</Button>\n\t\t<Title>$2</Title>\n\t\t<Button>\n\t\t\t<Icon name=\"$3\" />\n\t\t</Button>\n\t</Header>\n\n\t<Content>\n\t</Content>\n\n\t<Footer>\n\t\t<FooterTab>\n\t\t\t<Button>\n\t\t\t\t$4\n\t\t\t\t<Icon name=\"$5\" />\n\t\t\t</Button>\n\t\t\t<Button>\n\t\t\t\t$6\n\t\t\t\t<Icon name=\"$7\" />\n\t\t\t</Button>\n\t\t\t<Button>\n\t\t\t\t$8\n\t\t\t\t<Icon name=\"$9\" />\n\t\t\t</Button>\n\t\t\t<Button>\n\t\t\t\t$10\n\t\t\t\t<Icon name=\"$11\" />\n\t\t\t</Button>\n\t\t</FooterTab>\n\t</Footer>\n</Container>\n",
		"description": "Container containing Header, Content, Footer",
		"scope": "source.js"
	},
